<!DOCTYPE html>
<html lang="pt-BR">
<head>
    <meta charset="UTF-8">
</head>
<body>
    <%- include('cabecalho.ejs') %>
    <h1><%= nome %></h1>
    <section>
        <h1>Formações</h1>
        <ul>
            <% for (let formacao of formacoes) { %>
                <li><%= formacao %></li>
            <% } %>
        </ul>
    </section>
    <section>
        <h1>Experiências profissionais</h1>
        <ul>
            <% for (let experiencia of experiencias) { %>
                <li><%= experiencia %></li>
            <% } %>
        </ul>
    </section>
</body>
</html>
